    <operation name="consultarAsignaturasMatriculadas">
        <soap:operation soapAction="urn:ErasmusWSDL#consultarAsignaturasMatriculadas" />
        <input>
          <soap:body use="literal" namespace="urn:ErasmusWSDL"
